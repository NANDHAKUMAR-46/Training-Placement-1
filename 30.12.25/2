n=input()
t=int(input())
res=[]
def dfs(i,p,v):
    if i==len(n):
        if v==t:res.append(p)
        return
    for j in range(i+1,len(n)+1):
        s=n[i:j]
        if len(s)>1 and s[0]=="0":break
        x=int(s)
        if i==0:dfs(j,s,x)
        else:
            dfs(j,p+"+"+s,v+x)
            dfs(j,p+"-"+s,v-x)
            dfs(j,p+"*"+s,v-x+(v-(v-x))*x)
dfs(0,"",0)
print(res)
