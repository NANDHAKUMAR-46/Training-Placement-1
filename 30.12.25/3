m=[list(map(int,input().split())) for _ in range(int(input()))]
dp=[[0]*(len(m[0])+1) for _ in range(len(m)+1)]
mx=0
for i in range(1,len(dp)):
    for j in range(1,len(dp[0])):
        if m[i-1][j-1]:
            dp[i][j]=1+min(dp[i-1][j],dp[i][j-1],dp[i-1][j-1])
            mx=max(mx,dp[i][j])
print(mx*mx)
