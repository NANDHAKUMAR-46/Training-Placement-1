public class LinearSearch {
    public static void main(String[] args) {
        int[] arr = {10, 25, 30, 40};
        int key = 30;
        boolean found = false;
        for (int num : arr) {
            if (num == key) {
                found = true;
                break;
            }
        }
        System.out.println(key + (found ? " Found" : " Not Found"));
    }
}
