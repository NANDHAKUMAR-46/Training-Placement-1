public class Calculator {
    public static void main(String[] args) {
        int a = 10, b = 5;
        char op = '+';
        int result = 0;
        if (op == '+') result = a + b;
        else if (op == '-') result = a - b;
        else if (op == '*') result = a * b;
        else if (op == '/') result = a / b;
        else System.out.println("Invalid operator");
        System.out.println("Result: " + result);
    }
}
